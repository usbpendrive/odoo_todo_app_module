<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="index_template" name="My Todo List">
        <div id="wrap" class="container">
            <h1>Todo Tasks</h1>
            <t t-foreach="tasks" t-as="task">
                <div class="row">
                    <span t-field="task.name" />
                </div>
            </t>
        </div>
    </template>
</odoo>